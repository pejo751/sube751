<h1>Importaciones</h1>

<table border = '1' >
  <tr>
	<th>Id</th>
	<th>Archivo</th>
	<th>Tipo</th>
	<th>Tamanio</th>
	<th>Actualizacion</th>
  </tr>

<% @archivos.each do |a| %>
  <tr>
	<td><%=h a.id %></td>
	<td><%=h a.nombre_file_name %></td>
	<td><%=h a.nombre_content_type %></td>
	<td><%=h a.nombre_file_size %></td>
	<td><%=h a.nombre_updated_at %></td>
	<td><%= link_to 'elimiar', a, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<%= link_to 'Cancelar', clientes_path %>
